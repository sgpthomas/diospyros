{
    "constants": [],
    "seed_rules": [
	{
	    "lhs": "(+ ?a ?b)",
	    "rhs": "(+ ?b ?a)"
	},
	{
	    "lhs": "(* ?a ?b)",
	    "rhs": "(* ?b ?a)"
	},
	{
	    "lhs": "(+ ?a i0)",
	    "rhs": "?a"
	},
	{
	    "lhs": "(* ?a i1)",
	    "rhs": "?a"
	},
	{
	    "lhs": "(Vec (+ ?a ?b))",
	    "rhs": "(VecAdd (Vec ?a) (Vec ?b))"
	},
	{
	    "lhs": "(Vec (* ?a ?b))",
	    "rhs": "(VecMul (Vec ?a) (Vec ?b))"
	}
    ],
    "unops": [
	"neg"
    ],
    "binops": [
	"+",
	"*",
	"-"
    ],
    "use_vector": true,
    "vector_mac": true,
    "variable_duplication": false
}
